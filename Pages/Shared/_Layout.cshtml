<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ParcelLookup</title>
    <link rel="stylesheet" href="https://kingcounty.gov/en/assets/KCGov/css/styles.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ParcelLookup.styles.css" asp-append-version="true" />
</head>
<body id="mainBody" class="albacore mb-0">
    <a href="#maincontent" class="sr-only sr-only-focusable">Skip to main content</a>
    <header class="global-header mb-0">
        <div class="container-fluid">
            <div class="row">
                <div class="order-1 col-6 col-md-auto">
                    <a class="navbar-brand" href="https://kingcounty.gov/en/">
                        <img src="https://cds.kingcounty.gov/KClogo-black.gif" alt="King County Logo">
                    </a>
                </div>
                <div class="order-2 order-md-3 col-6 col-md-auto flex-md-fill d-flex d-md-flex justify-content-end">
                    <div class="dropdown language-selector align-middle d-flex">
                        <button class="btn btn-link dropdown-toggle disabled" type="button" id="languageMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            English <span class="language-selector-chevron float-right"></span>
                        </button>
                    </div>
                </div>
                <div class="order-3 order-md-2 col-md-auto p-0 w-auto flex-md-fill d-inline-flex">
                    <div>
                        <a>
                            <div class="header-date" aria-label="Today's Date is">
                                @{
                                    var shortMonth = DateTime.Now.ToString("MMMM")[..3].ToUpperInvariant();
                                    var day = @DateTime.Now.Day;
                                }
                                <span class="header-date-month">@shortMonth</span><hr><span class="header-date-day">@day</span>
                                <div class="header-date-arrow"></div>
                            </div>
                        </a>
                    </div>
                </div>
                <div class="order-5 d-inline-flex col-md-auto p-0 w-auto">
                    <nav aria-labeledby="globalNavigation">
                        <h2 id="globalNavigation" class="sr-only">Global Navigation</h2>
                        <div class="navbar float-right">
                            <button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#kcNav" aria-controls="navbarTogglerDemo02" aria-expanded="false" aria-label="Toggle navigation"><span></span><span></span><span></span></button>
                        </div>
                    </nav>
                </div>
            </div>
            <div class="row">
                <div class="navbar-collapse collapse" id="kcNav">
                    <ul class="nav navbar-nav">
                        <li class="nav-item"><a class="nav-link" href="/en/coming-soon">Register or Pay</a></li>
                        <li class="nav-item"><a class="nav-link" href="/en/records-requests">Public Records</a></li>
                        <li class="nav-item"><a class="nav-link" href="https://kingcounty.gov/elected/executive/constantine/initiatives/all-initiatives.aspx">Initiatives</a></li>
                        <li class="nav-item"><a class="nav-link" href="/en/coming-soon">Events</a></li>
                        <li class="nav-item"><a class="nav-link" href="/en/coming-soon">News</a></li>
                        <li class="nav-item"><a class="nav-link" href="https://kingcounty.gov/about/contact-us.aspx">Contact Us</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <script src="/en/assets/KCGov/scripts/cookies.js"></script>
        <div class="alert-wrapper">
        </div>
        <script type="text/javascript">
            function onKeyPressDisableMe(e, alertId) {
                if (e.keyCode == 32 || e.keyCode == 13) {
                    setCookie('KCGov-Alerts', alertId);
                    return false;
                }
            }
        </script>
    </header>
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
    <footer>
        <div class="index-strip">
            <div class="container">
                <ul class="">
                    <li class="d-none d-md-block"><a href="#">Index</a></li>
                    <li><a href="#A">A</a></li>
                    <li><a href="#B">B</a></li>
                    <li><a href="#C">C</a></li>
                    <li><a href="#D">D</a></li>
                    <li><a href="#E">E</a></li>
                    <li><a href="#F">F</a></li>
                    <li><a href="#G">G</a></li>
                    <li><a href="#H">H</a></li>
                    <li><a href="#I">I</a></li>
                    <li><a href="#J">J</a></li>
                    <li><a href="#K">K</a></li>
                    <li><a href="#L">L</a></li>
                    <li><a href="#M">M</a></li>
                    <li><a href="#N">N</a></li>
                    <li><a href="#O">O</a></li>
                    <li><a href="#P">P</a></li>
                    <li><a href="#Q">Q</a></li>
                    <li><a href="#R">R</a></li>
                    <li><a href="#S">S</a></li>
                    <li><a href="#T">T</a></li>
                    <li><a href="#U">U</a></li>
                    <li><a href="#V">V</a></li>
                    <li><a href="#W">W</a></li>
                    <li><a href="#X">X</a></li>
                    <li><a href="#Y">Y</a></li>
                    <li><a href="#Z">Z</a></li>
                </ul>
            </div>
        </div>
        <nav aria-labelledby="footerLinks" class="footer-nav">
            <div class="container">
                <h2 id="footerLinks" class="sr-only">Footer Links</h2>
                <div class="row">
                    <div class="col-md-3 d-flex align-items-center">
                        <div class="footer-social-links d-flex">
                            <a href="https://www.facebook.com/KingCountyWA">
                                <img src="https://kingcounty.gov/en/-/media/king-county/icons/social-media/fb_icon.png" alt="King County Facebook page">
                            </a>
                            <a href="https://www.linkedin.com/company/5871?trk=vsrp_companies_res_name&amp">
                                <img src="https://kingcounty.gov/en/-/media/king-county/icons/social-media/lin_icon.png" alt="King County LinkedIn page">
                            </a>
                            <a href="https://twitter.com/kingcountywa">
                                <img src="https://kingcounty.gov/en/-/media/king-county/icons/social-media/twit_icon.png" alt="King County Twitter page">
                            </a>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="link-list link-list-border link-list-border-left ">
                            <!-- <h3 class="link-list-title">Guidance</h3>-->
                            <ul>
                                <li><a href="https://kingcounty.gov/about/website/termsOfUse.aspx">Terms of Use</a></li>
                                <li><a href="https://kingcounty.gov/about/website/privacy.aspx">Privacy</a></li>
                                <li><a href="https://kingcounty.gov/about/website/Transparency.aspx">Data Transparency</a></li>
                            </ul>
                        </div>
                        <div class="link-list link-list-border link-list-border-left ">
                            <!-- <h3 class="link-list-title">Digital</h3>-->
                            <ul>
                                <li><a href="/en/coming-soon">Feedback</a></li>
                                <li><a href="https://kingcounty.gov/about/news/subscribe.aspx">Email or Text Alerts</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="link-list link-list-border link-list-border-left ">
                            <!-- <h3 class="link-list-title">About</h3>-->
                            <ul>
                                <li><a href="https://directory.kingcounty.gov/EmpSearch.asp">Staff Directory</a></li>
                                <li><a href="https://kingcounty.gov/elected.aspx">Elected Officials</a></li>
                                <li><a href="https://kingcounty.gov/about/contact-us/phone-directory.aspx">Phone List</a></li>
                                <li><a href="https://kingcounty.gov/about/contact-us/locations.aspx">Office Locations</a></li>
                                <li><a href="https://kingcounty.gov/about/contact-us/closures.aspx">Holidays and Closures</a></li>
                                <li><a href="https://kingcounty.gov/audience/employees.aspx">Employee Pages</a></li>
                                <li><a href="https://kingcounty.gov/audience/employees/careers.aspx">Careers</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="link-list link-list-border link-list-border-left">
                            <!-- <h3 class="link-list-title">Priorities</h3>-->
                            <ul>
                                <li><a href="https://kingcounty.gov/elected/executive/constantine/priorities/building-equity.aspx">Equity and Social Justice</a></li>
                                <li><a href="https://kingcounty.gov/elected/executive/constantine/priorities/census.aspx">2020 Census</a></li>
                                <li><a href="https://kingcounty.gov/elected/executive/constantine/priorities/best-run-government.aspx">Best-Run Government</a></li>
                                <li><a href="https://kingcounty.gov/elected/executive/constantine/priorities/climate-change.aspx">Climate Change</a></li>
                                <li><a href="https://kingcounty.gov/elected/executive/constantine/priorities/transportation.aspx">Regional Mobility</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
        <div class="footer-legal">
            <div class="container">
                <div class="row">
                    <div class="col-sm-8">
                        <ul>
                            <li><a href="#">Contact us</a></li>
                            <li><a href="#">Customer service</a></li>
                            <li><a href="#">Terms of use</a></li>
                            <li><a href="#">Privacy policy</a></li>
                        </ul>
                    </div>
                    <div class="col-sm-4">
                        <p>© King County, WA 2022</p>
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>